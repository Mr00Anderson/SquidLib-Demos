<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit trunk//EN" "https://gwt.googlesource.com/gwt/+/master/./distro-source/core/src/gwt-module.dtd">
<module rename-to="html">
	<inherits name='com.badlogic.gdx.backends.gdx_backends_gwt' />

	<inherits name='regexodus' />

	<inherits name='squidlib-util' />

	<!-- Comment this next line out if you don't depend on the text display part of SquidLib -->
	<inherits name='squidlib'/>

	<inherits name='TsarGame' />
	<entry-point class='com.squidpony.the.tsar.client.HtmlLauncher' />
	
	<set-configuration-property name="gdx.assetpath" value="../android/assets" />
	<set-configuration-property name='xsiframe.failIfScriptTag' value='FALSE'/>
</module>
